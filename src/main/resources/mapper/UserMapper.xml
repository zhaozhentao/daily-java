<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzt.daily.mapper.UserMapper">
  <select id="findByName" resultType="com.zzt.daily.mapper.User">
    select * from users
    <where>
      name = #{name}
    </where>
  </select>
</mapper>

